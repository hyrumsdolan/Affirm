{
  "name": "vite3-plugin-express",
  "version": "0.1.5",
  "description": "A vite plugin that hot reloads a node express server",
  "repository": {
    "type": "git",
    "url": "https://github.com/noam-honig/vite3-plugin-express"
  },
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./esm/index.js",
      "types": "./index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "release": "npm run build && npm publish --access public"
  },
  "license": "MIT",
  "author": {
    "name": "Noam Honig"
  },
  "files": [
    "esm/index.js",
    "esm/index.js.map",
    "esm/package.json",
    "index.d.ts",
    "index.js",
    "index.js.map",
    "index.ts",
    "package.json"
  ],
  "devDependencies": {
    "@types/node": "^18.11.9",
    "typescript": "^4.8.4",
    "vite": "^3.2.3"
  }
}